{
  "presets": [
    "stage-2",
    ["env", {
      "targets": {
        "browsers": ">= 1%"
      }
    }]
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-object-rest-spread",
    ["module-resolver", {
      "alias": {
        "config": "./src/app/config/index.js",
        "containers": "./src/app/containers",
        "services": "./src/app/services",
        "states": "./src/app/states/index.js",
        "polyfills": "./src/app/polyfills.js",
        "environment": "./environments/index.js",
      }
    }]
  ]
}
