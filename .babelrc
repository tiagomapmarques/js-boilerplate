{
  "presets": [
    ["env", {
      "targets": {
        "browsers": "ie >= 9"
      }
    }],
    "stage-2"
  ],
  "plugins": [
    ["module-resolver", {
      "alias": {
        "app": "./src/app"
      }
    }]
  ],
  "env": {
    "test": {
      "plugins": [
        ["module-resolver", {
          "alias": {
            "testing": "./src/testing",
            "app": "./src/app"
          }
        }]
      ]
    }
  }
}
