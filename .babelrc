{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": [
          "defaults",
          "ie >= 9"
        ]
      }
    }]
  ],
  "plugins": [
    ["babel-plugin-module-resolver", {
      "alias": {
        "components": "./src/app/components",
        "^services$": "./src/app/services/index.js"
      }
    }]
  ],
  "env": {
    "test": {
      "plugins": [
        ["babel-plugin-module-resolver", {
          "alias": {
            "^testing$": "./src/testing/index.js",
            "components": "./src/app/components",
            "^services$": "./src/app/services/index.js"
          }
        }]
      ]
    }
  }
}
