{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "defaults",
          "ie >= 9"
        ]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    ["module-resolver", {
      "alias": {
        "components": "./src/app/components"
      }
    }]
  ],
  "env": {
    "test": {
      "plugins": [
        ["module-resolver", {
          "alias": {
            "testing": "./src/testing",
            "components": "./src/app/components"
          }
        }]
      ]
    }
  }
}
