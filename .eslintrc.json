{
  "extends": "airbnb-base",
  "rules": {
    "import/no-unresolved": "\\.scss$",
    "import/prefer-default-export": "off",
    "import/no-default-export": "error"
  },
  "parser": "eslint-plugin-typescript/parser",
  "plugins": [
    "typescript"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "VARIABLES": true
  }
}
